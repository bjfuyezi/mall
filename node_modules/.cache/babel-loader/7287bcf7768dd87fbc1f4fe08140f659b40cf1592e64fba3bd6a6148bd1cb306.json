{"ast":null,"code":"export default {\n  name: 'RegisterView',\n  data() {\n    return {\n      registerForm: {\n        username: '',\n        phone: '',\n        smsCode: '',\n        password: '',\n        confirmPassword: '',\n        captcha: '',\n        agreement: false\n      },\n      countdown: 0,\n      captchaUrl: 'data:image/png;base64,xxx' // 这里替换为实际的验证码图片URL\n    };\n  },\n  methods: {\n    // 发送短信验证码\n    sendVerificationCode() {\n      // 验证手机号\n      if (!/^1[3-9]\\d{9}$/.test(this.registerForm.phone)) {\n        alert('请输入正确的手机号码');\n        return;\n      }\n\n      // 开始倒计时\n      this.countdown = 60;\n      const timer = setInterval(() => {\n        this.countdown--;\n        if (this.countdown <= 0) {\n          clearInterval(timer);\n        }\n      }, 1000);\n\n      // 这里调用发送验证码的API\n      console.log('发送验证码到：', this.registerForm.phone);\n    },\n    // 刷新图形验证码\n    refreshCaptcha() {\n      // 这里更新验证码图片\n      this.captchaUrl = `data:image/png;base64,xxx?t=${Date.now()}`;\n    },\n    // 处理注册\n    handleRegister() {\n      // 表单验证\n      if (!this.validateForm()) {\n        return;\n      }\n\n      // 这里处理注册逻辑\n      console.log('���单提交：', this.registerForm);\n    },\n    // 表单验证\n    validateForm() {\n      // 用户名验证\n      if (!/^[a-zA-Z0-9_]{4,16}$/.test(this.registerForm.username)) {\n        alert('用户名格式不正确');\n        return false;\n      }\n\n      // 手机号验证\n      if (!/^1[3-9]\\d{9}$/.test(this.registerForm.phone)) {\n        alert('请输入正确的手机号码');\n        return false;\n      }\n\n      // 密码验证\n      if (!/^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,20}$/.test(this.registerForm.password)) {\n        alert('密码格式不正确');\n        return false;\n      }\n\n      // 确认密码\n      if (this.registerForm.password !== this.registerForm.confirmPassword) {\n        alert('两次输入的密码不一致');\n        return false;\n      }\n      return true;\n    }\n  }\n};","map":{"version":3,"names":["name","data","registerForm","username","phone","smsCode","password","