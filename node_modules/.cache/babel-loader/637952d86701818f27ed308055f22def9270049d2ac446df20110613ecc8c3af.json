{"ast":null,"code":"export default {\n  props: {\n    isDialog: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data() {\n    return {\n      addresses: []\n      // ... 其他数据保持不变 ...\n    };\n  },\n  methods: {\n    selectAddress(address) {\n      if (this.isDialog) {\n        this.$emit('address-selected', address);\n      }\n    },\n    async loadAddresses() {\n      try {\n        const response = await this.$axios.get(`http://localhost:8081/addresses/user/${this.userId}`);\n        if (response.data.status === 'success') {\n          this.addresses = response.data.data;\n          this.$emit('address-updated', this.addresses);\n        }\n      } catch (error) {\n        console.error('获取地址列表失败:', error);\n        this.$message.error('获取地址列表失败');\n      }\n    },\n    // ... 其他方法保持不变 ...\n\n    async submitForm() {\n      this.$refs.addressForm.validate(async valid => {\n        if (valid) {\n          try {\n            // ... 提交表单逻辑保持不变 ...\n\n            // 成功后重新加载地址列表\n            await this.loadAddresses();\n          } catch (error) {\n            console.error('提交地址失败:', error);\n            this.$message.error(this.isEdit ? '更新失败' : '添加失败');\n          }\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["props","isDialog","type","Boolean","default","data","addresses","methods","selectAddress","address","$emit","loadAddresses","response","$axios","get","userId","status","error","console","$message","submitForm","$refs","addressForm","validate","valid","isEdit"],"sources":["src/views/UserAddress.vue"],"sourcesContent":["<template>\r\n  <div class=\"address-management\" :class=\"{ 'is-dialog': isDialog }\">\r\n    <!-- 地址列表 -->\r\n    <div class=\"address-list\">\r\n      <el-card shadow=\"never\">\r\n        <div slot=\"header\" class=\"card-header\">\r\n          <div class=\"header-left\">\r\n            <span class=\"title\">收货地址</span>\r\n          </div>\r\n          <el-button type=\"primary\" size=\"small\" @click=\"showAddDialog\">\r\n            <i class=\"el-icon-plus\"></i> 新增地址\r\n          </el-button>\r\n        </div>\r\n\r\n        <div class=\"address-items\">\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"24\" v-for=\"address in addresses\" :key=\"address.address_id\">\r\n              <el-card shadow=\"hover\" class=\"address-item\" @click.native=\"selectAddress(address)\">\r\n                <div class=\"address-content\">\r\n                  <div class=\"address-main\">\r\n                    <div class=\"location\" v-if=\"address.address_content\">\r\n                      <i class=\"el-icon-location\"></i>\r\n                      {{ address.province }} {{ address.address_content }}\r\n                    </div>\r\n                    <div class=\"phone\" v-if=\"address.phone\">\r\n                      <i class=\"el-icon-phone\"></i>\r\n                      {{ address.phone }}\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"address-tags\">\r\n                    <el-tag v-if=\"address.is_default === 1\" type=\"success\" size=\"small\">默认地址</el-tag>\r\n                  </div>\r\n                </div>\r\n                <div class=\"address-actions\">\r\n                  <el-button type=\"text\" @click.stop=\"handleEdit(address)\">编辑</el-button>\r\n                  <el-button type=\"text\" @click.stop=\"handleDelete(address)\" class=\"delete-btn\">删除</el-button>\r\n                  <el-button \r\n                    v-if=\"address.is_default !== 1\"\r\n                    type=\"text\" \r\n                    @click.stop=\"setDefault(address)\">\r\n                    设为默认\r\n                  </el-button>\r\n                </div>\r\n              </el-card>\r\n            </el-col>\r\n          </el-row>\r\n        </div>\r\n      </el-card>\r\n    </div>\r\n\r\n    <!-- 添加/编辑地址对话框 -->\r\n    <el-dialog :title=\"dialogTitle\" :visible.sync=\"dialogVisible\">\r\n      <el-form :model=\"addressForm\" :rules=\"rules\" ref=\"addressForm\" label-width=\"100px\">\r\n        <!-- ... 表单内容保持不变 ... -->\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: {\r\n    isDialog: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      addresses: [],\r\n      // ... 其他数据保持不变 ...\r\n    }\r\n  },\r\n  methods: {\r\n    selectAddress(address) {\r\n      if (this.isDialog) {\r\n        this.$emit('address-selected', address);\r\n      }\r\n    },\r\n\r\n    async loadAddresses() {\r\n      try {\r\n        const response = await this.$axios.get(`http://localhost:8081/addresses/user/${this.userId}`);\r\n        if (response.data.status === 'success') {\r\n          this.addresses = response.data.data;\r\n          this.$emit('address-updated', this.addresses);\r\n        }\r\n      } catch (error) {\r\n        console.error('获取地址列表失败:', error);\r\n        this.$message.error('获取地址列表失败');\r\n      }\r\n    },\r\n\r\n    // ... 其他方法保持不变 ...\r\n\r\n    async submitForm() {\r\n      this.$refs.addressForm.validate(async (valid) => {\r\n        if (valid) {\r\n          try {\r\n            // ... 提交表单逻辑保持不变 ...\r\n            \r\n            // 成功后重新加载地址列表\r\n            await this.loadAddresses();\r\n          } catch (error) {\r\n            console.error('提交地址失败:', error);\r\n            this.$message.error(this.isEdit ? '更新失败' : '添加失败');\r\n          }\r\n        }\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.address-management {\r\n  &.is-dialog {\r\n    padding: 0;\r\n    \r\n    .address-item {\r\n      cursor: pointer;\r\n      \r\n      &.active {\r\n        border-color: #409EFF;\r\n        background-color: #ecf5ff;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style> "],"mappings":"AAgEA;EACAA,KAAA;IACAC,QAAA;MACAC,IAAA,EAAAC,OAAA;MACAC,OAAA;IACA;EACA;EACAC,KAAA;IACA;MACAC,SAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,cAAAC,OAAA;MACA,SAAAR,QAAA;QACA,KAAAS,KAAA,qBAAAD,OAAA;MACA;IACA;IAEA,MAAAE,cAAA;MACA;QACA,MAAAC,QAAA,cAAAC,MAAA,CAAAC,GAAA,8CAAAC,MAAA;QACA,IAAAH,QAAA,CAAAP,IAAA,CAAAW,MAAA;UACA,KAAAV,SAAA,GAAAM,QAAA,CAAAP,IAAA,CAAAA,IAAA;UACA,KAAAK,KAAA,yBAAAJ,SAAA;QACA;MACA,SAAAW,KAAA;QACAC,OAAA,CAAAD,KAAA,cAAAA,KAAA;QACA,KAAAE,QAAA,CAAAF,KAAA;MACA;IACA;IAEA;;IAEA,MAAAG,WAAA;MACA,KAAAC,KAAA,CAAAC,WAAA,CAAAC,QAAA,OAAAC,KAAA;QACA,IAAAA,KAAA;UACA;YACA;;YAEA;YACA,WAAAb,aAAA;UACA,SAAAM,KAAA;YACAC,OAAA,CAAAD,KAAA,YAAAA,KAAA;YACA,KAAAE,QAAA,CAAAF,KAAA,MAAAQ,MAAA;UACA;QACA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}